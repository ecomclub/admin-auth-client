  },
  "homepage": "https://github.com/ecomclub/ecomplus-auth#readme",
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "@ecomplus/client": "^1.1.3",
    "@ecomplus/utils": "^1.1.1",
